{
  "name": "publiccode-editor",
  "version": "1.4.3",
  "license": "AGPL-3.0-or-later",
  "description": "[License](./LICENSE)",
  "scripts": {
    "dev": "webpack serve --mode development",
    "lint": "eslint --ext .js,jsx,.ts,.tsx --max-warnings 0 .",
    "build:licenses": "mkdir -p src/generated && ts-node --cwd scripts genLicenseList.ts src/generated/licenses.json",
    "build:wasm": "cd src && mkdir -p generated && cp \"$(go env GOROOT)/misc/wasm/wasm_exec.js\" generated && GOOS=js GOARCH=wasm go build -o generated/main.wasm wasm/main.go",
    "build:dev": "webpack --mode development",
    "build:app": "webpack --mode production",
    "build": "npm run build:licenses && npm run build:wasm && npm run build:app",
    "test": "jest",
    "deploy": "gh-pages -u 'Deploy Bot <no-reply@teamdigitale.governo.it>' -d dist",
    "postinstall": "patch-package",
    "release": "release-it"
  },
  "keywords": [
    "publiccode",
    "editor"
  ],
  "author": "Lorenzo Ponticelli <point.point@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:italia/publiccode-editor.git"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/svgImportMock.js",
      "^worker-loader!.*": "<rootDir>/src/__mocks__/workerMock.js"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "devDependencies": {
    "@release-it/bumper": "^4.0.2",
    "@release-it/conventional-changelog": "^5.1.1",
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/golang-wasm-exec": "^1.15.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/react-dom": "^17.0.20",
    "@types/validator": "^13.7.17",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "10.4.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.4",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "faker": "^5.5.3",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "import-glob-loader": "^1.1.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.7.6",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "process": "^0.11.10",
    "react-test-renderer": "^17.0.2",
    "release-it": "^15.11.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "bootstrap": "^4.6.0",
    "bootstrap-italia": "^2.5.1",
    "classnames": "^2.3.2",
    "cldr-data": "^36.0.1",
    "clean-deep": "^3.4.0",
    "copy-to-clipboard": "^3.3.3",
    "deepmerge": "^4.3.1",
    "design-react-kit": "^5.0.0-0",
    "dotenv": "^16.3.1",
    "draft-js": "^0.11.7",
    "i18next": "^23.2.2",
    "immutable": "^4.3.0",
    "js-yaml": "3.14.0",
    "lodash": "^4.17.21",
    "prop-type": "^0.0.1",
    "rc-collapse": "^3.7.0",
    "react": "^17.0.2",
    "react-collapsible": "^2.10.0",
    "react-dom": "^17.0.2",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^6.15.4",
    "react-i18next": "^13.0.0",
    "react-jss": "^10.10.0",
    "react-phone-number-input": "^3.2.23",
    "react-redux": "^8.1.1",
    "react-rte": "^0.16.3",
    "react-widgets": "^5.8.4",
    "slugify": "^1.6.6",
    "updeep": "^1.2.4",
    "validator": "^13.9.0"
  }
}
